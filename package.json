  "author": "LuisLiraC <luisliradev@gmail.com> (https://luislira.xyz/)",
  "license": "ISC",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "mongodb": "3.5.5",
    "pug": "3.0.1"
  },
  "devDependencies": {
    "nodemon": "1.19.1",
    "yargs": "13.2.4"
  }
}
